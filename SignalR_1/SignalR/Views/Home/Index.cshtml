@model IEnumerable<AppUser>

<link href="~/css/style.css" rel="stylesheet" />

    <div class="page-content page-container" id="page-content">
        <div class="padding">
            <div class="row">
                <div class="col-sm-6">


                    @foreach (var item in Model)
                    {
                        <div class="list list-row block">
                            <div class="list-item" data-id="19">
                                <div>
                                    <a href="#" data-abc="true">
                                        <span class="w-48 avatar gd-warning">S</span>
                                    </a>
                                </div>
                                <div class="flex">
                                    <a asp-action="SelectedMessage" asp-route-receverId="@item.Id"  class="item-author text-color" data-abc="true">
                                        @item.UserName
                                    </a>
                                    <div class="item-except text-muted text-sm h-1x">@item.Email</div>
                                </div>
                                <div class="no-wrap">
                                    <div class="item-date text-muted text-sm d-none d-md-block">13/12 18</div>
                                </div>
                            </div>

                        </div>
                    }
                </div>

            </div>
        </div>
    </div>